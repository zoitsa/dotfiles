# Shortcuts
alias ...='../..'
alias zrc="vim ~/.zshrc"
alias vrc="vim ~/.vimrc"

alias vinstall="vim +PluginInstall +qall"
alias :q="exit"
alias wds="webpack-dev-server"
# Include custom aliases
[[ -f ~/.aliases.local ]] && source ~/.aliases.local
alias t="todo.sh"
alias ga="git add -A"
alias gc="git commit"
alias gl="git pull"
alias gh="git push"
alias gd="git diff"
alias gs="git status"
alias glog="git log"
alias simpleserver="python -m SimpleHTTPServer"
